model user{
    id Int @default(autoincrement())
    name String
    username String?
    role Role
    post post[]
    CreatedAt DateTime @default(now())
    updatedAt DateTime @updatedAt

}

model post{
    id Int @default(autoincrement())
    userId Int
    user User @relation(fields:[userID],references:[id])
}

enum Role{
    Admin
    user
}