
datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         Int       @id @default(autoincrement())
  university String
  profile Profile?
}

model Profile {
  id      Int    @id @default(autoincrement())
  email   String
  userId  Int @unique
  name String
  user User @relation(fields: [userId], references: [id])
}
